#include <main.h>
#include "Flex_LCD420.c"
#USE RS232(BAUD=9600, xmit=PIN_C6, rcv=PIN_C7,STREAM=COM_A)
#USE RS232(BAUD=9600, xmit=PIN_C1, rcv=PIN_C2,STREAM=COM_b)

void main()
{
   lcd_init(); //initialize our lcd
   char c;
   
   
   
   while(TRUE)
   {
      //puts("HELLO");
      c = fgetc(COM_A);
      lcd_gotoxy(1,1);
      printf(LCD_PUTC," %C ",c);

      //TODO: User Code
   }

}
